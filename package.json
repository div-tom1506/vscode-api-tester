{
  "name": "vscode-api-tester",
  "displayName": "vscode api tester",
  "description": "The API Tester Extension is a lightweight alternative to Postman, built directly into VS Code. It allows developers to test REST APIs without leaving their coding environment.",
  "version": "1.0.5",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages",
    "Testing"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "apiTester.open",
        "title": "api tester"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "compile": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.2"
  },
  "author": {
    "name": "Divyanshu Kumar"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/div-tom1506/vscode-api-tester-extension"
  },
  "publisher": "div-tom1506"
}
